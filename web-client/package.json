{
  "name": "web-client",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Alabe Duarte",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html -p 3000",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watchAll --verbose false",
    "lint": "eslint src",
    "format": "prettier-eslint \"**/*.js\" --write"
  },
  "dependencies": {
    "chart.js": "^2.7.3",
    "parcel-bundler": "^1.10.3",
    "react": "^16.6.0",
    "react-chartkick": "^0.3.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^21.27.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eventsource": "^1.0.7",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "nock": "^10.0.2",
    "node-fetch": "^2.6.0",
    "prettier-eslint-cli": "^4.7.1",
    "regenerator-runtime": "^0.12.1",
    "sass": "^1.23.3",
    "synchronous-promise": "^2.0.10"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**"
    ],
    "roots": [
      "src"
    ],
    "cacheDirectory": ".tmp",
    "moduleNameMapper": {
      "^test-support$": "<rootDir>/test-support",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-support/enzyme-adapter-setup.js",
    "setupFiles": [
      "<rootDir>/test-support/fetch.js"
    ]
  }
}
